    <validationRules>
        <fullName>Change_Contract_Date</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL( Extend_Contract_End_Date__c ,&quot;Yes&quot;) ,  ISNULL( New_Contract_End_Date__c ))</errorConditionFormula>
        <errorDisplayField>New_Contract_End_Date__c</errorDisplayField>
        <errorMessage>Please enter in a new contract end date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Change_Services_Completed_by_Date</fullName>
        <active>true</active>
        <description>If user elects to change services to be completed by date, they must enter a new date.</description>
        <errorConditionFormula>AND(ISPICKVAL( extend_services_to_be_completed_by__c ,&quot;Yes&quot;) ,  ISNULL(  New_Services_to_be_Completed_By_Date__c ))</errorConditionFormula>
        <errorDisplayField>New_Services_to_be_Completed_By_Date__c</errorDisplayField>
        <errorMessage>Please enter a new services to be completed by date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_for_Contract_End_Date</fullName>
        <active>true</active>
        <description>Changing the contract end date with the amendment, requires that an end date already exist in the contract.  Otherwise, it will error on the expiration notification date when the end date is set upon approval.</description>
        <errorConditionFormula>AND(ISPICKVAL( Extend_Contract_End_Date__c ,&quot;Yes&quot;) ,  ISNULL(  Contract_Number__r.EndDate  ))</errorConditionFormula>
        <errorDisplayField>New_Contract_End_Date__c</errorDisplayField>
        <errorMessage>Original contract does not have an end date defined. Check original contract and see if you need to extend the Services to be Completed by Date instead.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>NoCannotBeSelectedIfNewDateOrAmount</fullName>
        <active>true</active>
        <description>&apos;No&apos; cannot be selected if there is a new services to be completed by date, new contract end date, or new contract amount.</description>
        <errorConditionFormula>OR(AND(TEXT(extend_services_to_be_completed_by__c) = &apos;No&apos;, NOT(ISBLANK(New_Services_to_be_Completed_By_Date__c))),
AND(TEXT(Change_the_Contract_Amount__c) = &apos;No&apos;, Change_Contract_Amount_By__c&gt;0),
AND(TEXT(Extend_Contract_End_Date__c) = &apos;No&apos;, NOT(ISBLANK(New_Contract_End_Date__c))))</errorConditionFormula>
        <errorMessage>&apos;No&apos; cannot be selected in the corresponding field on the left side of the page if there is a new services to be completed by date, new contract end date, or new contract amount on the right side of the page.</errorMessage>
    </validationRules>
