IF(Operations__c =&quot;&quot;,2, 
IF(Project_Director_Program_Manager__c =&quot;&quot;,3,
IF(Senior_Project_Director__c =&quot;&quot;,4,
IF(Program_Director__c =&quot;&quot;,5,
