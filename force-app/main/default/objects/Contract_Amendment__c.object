    <fields>
        <fullName>Amended_Contract_Amount__c</fullName>
        <externalId>false</externalId>
        <formula>IF (Amended_Acres__c  = 0 &amp;&amp; Amended_Price_Per_Acre__c  &gt; 0, Contract_Number__r.Original_Acreage__c * Amended_Price_Per_Acre__c, 

IF(Amended_Acres__c &gt; 0 &amp;&amp; Amended_Price_Per_Acre__c = 0, Amended_Acres__c * Contract_Number__r.Price_Per_Acre__c,

IF(Amended_Acres__c &gt; 0 &amp;&amp; Amended_Price_Per_Acre__c  &gt;  0, Amended_Acres__c * Amended_Price_Per_Acre__c, 0 
)))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>Read-only field that multiples the amended acres by the amended price per acre.</inlineHelpText>
        <label>Amended Contract Amount</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Currency</type>
