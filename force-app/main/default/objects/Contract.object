    <fields>
        <fullName>Acre_Adjusted_Amount__c</fullName>
        <externalId>false</externalId>
        <formula>if(Adjusted_Acres__c&gt;0,Price_Per_Acre__c * Adjusted_Acres__c,
     if(Amended_Acres__c&gt;0,Amended_Contract_Amount__c,
         Bid_Contract_Amount__c))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>Read only formula field that calculates the adjusted acres multiplied by the amount per acre.</inlineHelpText>
        <label>Acre Adjusted Amount</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <type>Currency</type>
    </fields>
