    <fields>
        <fullName>Acres_Formula__c</fullName>
        <description>Copy of acres from property interest then saved to acres_final to be memorialized in the monitoring report when report is approved.</description>
        <externalId>false</externalId>
        <formula>IF(Property_Interest__c &lt;&gt;&quot;&quot;, Property_Interest__r.Tract_Acres__c,
IF(Property_Interest_Eco_monitoring__c &lt;&gt; &quot;&quot;, Property_Interest_Eco_monitoring__r.Tract_Acres__c,
IF(Property_Management__c &lt;&gt; &quot;&quot;, Property_Management__r.Total_Fee_Acres__c,0)))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>The number of acres that the interest covers is not editable on the monitoring report.  If for any reason this needs to change, it must be changed on the interest record.</inlineHelpText>
        <label>Acres</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
