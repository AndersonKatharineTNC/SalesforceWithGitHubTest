    <fields>
        <fullName>Acreage_Difference__c</fullName>
        <externalId>false</externalId>
        <formula>Acres__c  -  GIS_Acres__c</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>Difference between bid acres and GIS acres.</inlineHelpText>
        <label>Acreage Difference</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Acres_Percentage_Difference__c</fullName>
        <externalId>false</externalId>
        <formula>IF(GIS_Acres__c&gt;0,((Acres__c - GIS_Acres__c )/ GIS_Acres__c),1)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>% difference between Bid Acres and GIS Acres.</inlineHelpText>
        <label>Acreage Difference %</label>
        <precision>18</precision>
        <required>false</required>
        <scale>1</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Percent</type>
    </fields>
