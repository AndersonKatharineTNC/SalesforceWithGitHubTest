    <validationRules>
        <fullName>Easements_belong_in_Partial_Interest_PMU</fullName>
        <active>true</active>
        <errorConditionFormula>AND( RecordType.Name = &apos;Easement&apos;,
 ISPICKVAL(Ownership_Status__c,&apos;TNC Held Easement or Deed Restriction&apos;),
  Property_Interest_Group__r.RecordType.Name  &lt;&gt; &apos;Partial Interests&apos;)</errorConditionFormula>
        <errorDisplayField>Property_Interest_Group__c</errorDisplayField>
        <errorMessage>Easements should be added only to Property Interest Groups of type &quot;Partial Interests.&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Fees_belong_in_Fee_PMUs</fullName>
        <active>true</active>
        <errorConditionFormula>AND( RecordType.Name = &quot;Fee&quot;,  Property_Interest_Group__r.RecordType.Name  &lt;&gt; &quot;Fee&quot;,
 ISPICKVAL(Ownership_Status__c,&quot;TNC Owned Fee&quot;))</errorConditionFormula>
        <errorMessage>Fee property interests should be added only to Fee Property Interest Groups or Property Management Units.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Number_Subdivision_Cannot_exceed_permit</fullName>
        <active>true</active>
        <description>The number of subdivisions cannot exceed the number permitted.</description>
        <errorConditionFormula>AND( RecordType.Name = &apos;Easement&apos;, 
      Number_Subdivisions__c  &gt;  Number_Subdivisions_Permitted__c )</errorConditionFormula>
        <errorDisplayField>Number_Subdivisions__c</errorDisplayField>
        <errorMessage>The number of subdivisions cannot exceed the number of subdivisions permitted.</errorMessage>
    </validationRules>
